    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gridghost.mobile",
      "config": {
        "googleMapsApiKey": "AIzaSyCKBUC9ZTb7Dp-LHSgNh212EBBj6rmFauI"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dash-icons/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.gridghost.mobile",
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0",
      "config": {
