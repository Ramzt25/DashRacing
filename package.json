{
  "name": "gridghost",
  "version": "1.0.0",
  "description": "GridGhost Racing Platform - Race Anywhere, Meet Anywhere",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --project src/tsconfig.json",
    "start": "node dist/index.js",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "node dist/seed.js",
    "predeploy": "npm run build && npm run db:generate",
    "deploy": "azd up",
    "postdeploy": "azd logs"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "eslint": "^8.57.0",
    "jest": "^30.0.5",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/sensible": "^6.0.3",
    "@prisma/client": "^6.14.0",
    "@sinclair/typebox": "^0.34.40",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "argon2": "^0.44.0",
    "fastify": "^5.5.0",
    "geojson": "^0.5.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.14.0",
    "zod": "^4.1.1"
  },
  "packageManager": "npm@10.5.0",
  "engines": {
    "node": ">=18"
  },
  "keywords": ["racing", "social", "mobile", "gps", "automotive", "multiplayer"],
  "author": "GridGhost Team",
  "license": "MIT"
}
